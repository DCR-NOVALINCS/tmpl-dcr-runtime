# Notes: 
# - 0...3 returns a list with 4 elements: [0, 1, 2, 3]

# Previous syntax

%(a: A)[0]
%(b: B)[?]
(c: C)[?: Number] - when a.value > 1 | foreach i in 0...3 
(d: D)[?: String] - when a.value <= 1

c -->% a - when a.value > 1 | foreach i in 0...3
c -->+ b - when a.value > 1 | foreach i in 0...3

d -->+ a - when a.value <= 1
d -->% b - when a.value <= 1

# Alternative syntax

%(a: A)[0]
%(b: B)[?]

if (a.value > 1) 
    foreach i in 0...3
        (c: C)[?: Number]
        c -->% a
        c -->+ b
    /foreach
else 
    (d: D)[?: String]
    
    d -->+ a
    d -->% b
/if

