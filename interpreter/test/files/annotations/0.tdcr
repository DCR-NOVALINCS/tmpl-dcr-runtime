%(a: A)[0]
%(b: B)[?]

if a.value > 1 {
    foreach i in @range(0, 3) {
        (c: C)[?: Number]
        c -->% a
        c -->+ b
    }
} else {
    if a.value > 0 {
        (e: E)[?: Boolean]
        e --><> e
    }
    
    if a.value <= 1 AND a.value > 0 {
        (d: D)[?: String]
        d -->+ a
        d -->% b
    }
}