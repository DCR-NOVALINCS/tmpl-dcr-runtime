# (n: N)[i + 1] - foreach i in [0, 1, 2, 3]
# (b: B)[b] - foreach b in [true, false, true AND 1+1 > 0]
# (s: S)[s] - foreach s in ['a', 'b', 'c', 'd', 'e']

foreach i in @range(0, 3):
    (n: N)[i + 1]
/foreach

foreach b in [true, false, true AND 1+1 > 0]:
     (b: B)[b]
/foreach

foreach s in ["a", "b", "c", "d", "e"]:
    (s: S)[s]
/foreach

foreach r in [{ name: "a", value: 1 }, { name: "b", value: 2 }, { name: "c", value: 3 }]:
    (sr: S)[r.name]
    (nr: N)[r.value]
/foreach

foreach l in [@range(0, 2), [1, -1]]:
    foreach i in l:
        (li: N)[i]
    /foreach
/foreach