tmpl i1(a: A): B {
    (b: B)[0]

    a -->% b
} => b

tmpl i2(b: B): A {
    (a: A)[true]

    b -->% a
} => a

i1(a = a_i2) => b_i1
i2(b = b_i1) => a_i2
